{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": "Ticket types for events"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "can_be_listed": {
      "type": "boolean",
      "default": true
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "products"
    },
    "batches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::batch.batch",
      "mappedBy": "product"
    }
  }
}
